#include<iostream>
#include<vector>
#include<list>
using namespace std;
int main()
{
    list<int>l;
    l.push_back(1);
    l.push_back(2);
    l.push_back(3);
    l.push_back(4);
    l.push_front(0);
    int s=l.size();
    for(int i=0;i<s;i++)
    {
        cout<<l.front()<<" ";
        l.pop_front();
    }
    return 0;
}